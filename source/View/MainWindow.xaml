<appbar:AppBarWindow x:Class="Sidecab.View.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:appbar="clr-namespace:WpfAppBar"
    xmlns:lc="clr-namespace:Sidecab.View"
    mc:Ignorable="d"
    TextElement.FontWeight="Medium"
    TextElement.FontSize="14"
    FontFamily="{materialDesign:MaterialDesignFont}"
    Title="MainWindow" Height="450" Width="800" MinWidth="1"
    MouseEnter="AppBarWindow_MouseEnter" MouseDown="AppBarWindow_MouseDown">

</appbar:AppBarWindow>
